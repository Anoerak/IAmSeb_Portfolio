		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
	</head>
	<body>
		{# We place the flash messages here #}
		{% for message in app.flashes('danger') %}
			<div class="alert alert-danger" role="alert">
				<strong>Arf!</strong>
				{{ message }}
			</div>
		{% endfor %}
		{% for message in app.flashes('success') %}
			<div class="alert alert-success" role="alert">
				<strong>Nice!</strong>
				{{ message }}
			</div>
		{% endfor %}

		{# We place the Header component here #}
		{% block header %}
			{% include 'components/header/header.html.twig' %}
		{% endblock header %}

		{# We place the content of the page here #}
		{% block body %}{% endblock %}

		{# We place the Footer component here #}
		{% block footer %}
			{% include 'components/footer/footer.html.twig' %}
		{% endblock footer %}
	</body>
